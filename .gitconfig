[core]
    editor = vim
[diff]
    tool = vimdiff
[push]
    default = simple
[alias]
    br      = branch
    ci      = commit
    cia     = commit --amend
    co      = checkout
    cp      = cherry-pick
    hist    = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    lol     = log --abbrev-commit --decorate --graph --pretty=oneline
    lola    = log --abbrev-commit --all --decorate --graph --pretty=oneline
    pr      = pull --rebase
    rb      = rebase
    rbi     = rebase --interactive
[user]
	name = Guzman
	email = guzman@guzman.fi
[color]
	ui = auto
